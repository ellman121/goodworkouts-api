{
  "name": "goodworkoutsapi",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "dev": "run-p dev:watch dev:watchAliases dev:serve",
    "dev:watch": "tsc -w",
    "dev:watchAliases": "tsc-alias -w -p tsconfig.json",
    "dev:serve": "nodemon --exec \"npm run dev:lint > /dev/null && node build/index.js\"",
    "dev:lint": "eslint ./src 1>&2 && echo 'Linting successful' 1>&2",
    "test": "jest",
    "start": "node build/index.js",
    "migrations:create": "npx sequelize-cli db:migrate",
    "migrations:down": "npx sequelize-cli db:migrate:undo",
    "migrations:up": "npx sequelize-cli db:migrate"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@eslint/js": "^9.9.1",
    "@types/eslint__js": "^8.42.3",
    "@types/uuid": "^10.0.0",
    "ajv": "^8.17.1",
    "body-parser": "^1.20.2",
    "eslint-config-prettier": "^9.1.0",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "nodemon": "^3.1.4",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "ramda": "^0.30.1",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "tsc-alias": "^1.8.10",
    "typescript-eslint": "^8.3.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/ramda": "^0.30.1",
    "eslint": "^9.9.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "sequelize-cli": "^6.6.2",
    "typescript": "^5.5.4"
  }
}
